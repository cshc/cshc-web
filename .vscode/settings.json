{
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "javascript.validate.enable": false,
  "javascript.format.enable": false,
  "prettier.eslintIntegration": true,
  "python.venvPath": "${workspaceRoot}/venv",
  "python.pythonPath": "${workspaceRoot}/venv/Scripts/python.exe",
  "python.autoComplete.extraPaths": [
    "${workspaceRoot}/venv/Lib/site-packages",
    "${workspaceRoot}/src/frontend/js"
  ],
  "python.linting.pylintArgs": ["--load-plugins", "pylint_django"],
  "git.ignoreLimitWarning": true,
  "files.exclude": {
    "**/__pycache__": true,
    "node_modules": true,
    ".vscode": true,
    "venv": true,
    "**/.pyc": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "flow-typed": true
  },
  "html.format.enable": true,
  "restructuredtext.linter.executablePath": "${workspaceRoot}/venv/Scripts/restructuredtext-lint.exe",
  "restructuredtext.linter.run": "off",
  "restructuredtext.builtDocumentationPath": "${workspaceRoot}/docs/_build/html",
  "restructuredtext.confPath": "c:\\cshcV3\\cshc-web/docs",
  "restructuredtext.updateOnTextChanged": "true",
  "restructuredtext.sphinxBuildPath": "${workspaceRoot}/venv/Scripts",
  "html.format.wrapLineLength": 200
}
